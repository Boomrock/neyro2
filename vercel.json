{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py"
    }
  ],
  "env": {
    "PYTHONPATH": "/var/task",
    "STABILITY_API_KEY": "@stability_api_key",
    "OPENAI_API_KEY": "@openai_api_key"
  }
}